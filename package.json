{
  "name": "prototype_shell",
  "version": "1.0.0",
  "description": "A prototyping shell",
  "main": "index.js",
  "scripts": {
    "start": "PORT=5000 foreman start",
    "watch": "foreman start -f dev.procfile",
    "sync": "node_modules/browser-sync/bin/browser-sync start --proxy localhost:5000 --port 5001 --files 'public/*.html, public/*.css, public/*.js'",
    "bundle:app": "cd bundle && node ../node_modules/webpack/bin/webpack -w --config app.webpack.config.js",
    "install": "node_modules/napa/bin/napa",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-runtime": "^5.2.6",
    "body-parser": "^1.6.5",
    "cookie-parser": "^1.3.2",
    "datejs": "^1.0.0-rc3",
    "debug": "^1.0.4",
    "diskdb": "^0.1.14",
    "express": "^4.11.0",
    "file-loader": "^0.8.1",
    "is-mobile": "^0.2.2",
    "jade": "^1.9.1",
    "jquery": "^1.11.2",
    "morgan": "^1.2.3",
    "napa": "^1.2.0",
    "react": "^0.12.2",
    "url-loader": "^0.5.5"
  },
  "devDependencies": {
    "babel-loader": "^5.0.0",
    "browser-sync": "^1.9.0",
    "css-loader": "^0.8.0",
    "lodash": "^3.8.0",
    "style-loader": "^0.8.0",
    "webpack": "^1.4.15"
  },
  "napa": {
    "console-polyfill": "paulmillr/console-polyfill"
  },
  "engines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brianswisher/prototype_shell"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "author": "Brian Swisher<bswisher@gmail.com>",
  "license": "MIT"
}
